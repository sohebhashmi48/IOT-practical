// C++ code
//
int LED1=7;
int LED2=A1;
void setup()
{
  pinMode(LED1, OUTPUT);
  pinMode(LED2, OUTPUT);
}

void loop()
{
  digitalWrite(LED1, HIGH);
  digitalWrite(LED2, LOW);
  delay(1000); // Wait for 1000 millisecond(s)
  digitalWrite(LED1, LOW);
  digitalWrite(LED2, HIGH);
  delay(1000); // Wait for 1000 millisecond(s)
}
